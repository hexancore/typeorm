[mysqld]
performance_schema = off
# Ograniczenie pamiÄ™ci
innodb_buffer_pool_size = 16M
innodb_log_buffer_size = 1M
innodb_flush_log_at_trx_commit = 2

max_connections = 5
thread_cache_size = 2
table_open_cache = 32
query_cache_size = 0
query_cache_type = 0
key_buffer_size = 10M

thread_stack = 512k

skip-host-cache
skip_name_resolve
max_allowed_packet = 1M
tmp_table_size = 8M
max_heap_table_size = 8M

[mysqld_safe]
malloc-lib=/usr/lib/libjemalloc.so.1

[client]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4
