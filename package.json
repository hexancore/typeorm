{
  "name": "@hexancore/typeorm",
  "version": "0.12.0",
  "engines": {
    "node": ">=20"
  },
  "engine-strict": true,
  "description": "Hexancore typeorm support package",
  "author": "Andrzej Wasiak<Mararok>",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./lib/index.d.ts"
      ],
      "testutil": [
        "./lib/Test/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./testutil": {
      "import": {
        "types": "./lib/Test/index.d.ts",
        "default": "./lib/Test/index.js"
      },
      "default": {
        "types": "./lib/Test/index.d.ts",
        "default": "./lib/Test/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexancore/typeorm.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://hexancore.github.io/hexancore/",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --config .jestrc.json  --runInBand",
    "test:clearCache": "jest --config .jestrc.json  --clearCache",
    "test:unit": "jest --config .jestrc.json --runInBand --group=unit",
    "test:watch": "jest --config .jestrc.json  --runInBand --watchAll",
    "test:cov": "jest --config .jestrc.json  --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config .jestrc.json  --runInBand",
    "prepublish": "yarn lint && yarn test && rm -fr ./lib && yarn run build"
  },
  "peerDependencies": {
    "@hexancore/core": "^0.13.1",
    "mariadb": "^3.2.3",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "typeorm": "^0.3.19"
  },
  "peerDependenciesMeta": {
    "mariadb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  },
  "dependencies": {
    "@hexancore/common": "^0.13.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@hexancore/core": "^0.13.1",
    "@hexancore/mocker": "^1.1.2",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.5",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/testing": "^10.3.0",
    "@swc/core": "^1.3.101",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@types/jest": "27.0.*",
    "@types/node": "^20.0",
    "@types/pg": "^8",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "bull": "^4.12.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^10.1.0",
    "jest": "^29.7.0",
    "jest-docblock": "27.0.*",
    "jest-expect-message": "^1.1.3",
    "jest-runner": "^29.7.0",
    "jest-runner-groups": "^2.2.0",
    "mariadb": "^3.2.3",
    "mysql2": "^3.6.5",
    "nestjs-cls": "^3.6.0",
    "pg": "^8.11.3",
    "prettier": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typeorm": "^0.3.19",
    "typescript": "5.3.3"
  },
  "bugs": {
    "url": "https://github.com/hexancore/typeorm/issues"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "packageManager": "yarn@4.0.2"
}
